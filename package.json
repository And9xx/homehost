{
  "name": "homehost",
  "version": "1.0.0",
  "author": "ridhwaans",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=dev node server",
    "client-dev": "cd client && yarn dev",
    "start-dev": "concurrently --kill-others-on-fail \"yarn dev\" \"yarn client-dev\"",
    "postinstall": "cd client && npm install",
    "prestart": "cd client && yarn build",
    "start": "NODE_ENV=prod node server"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "chokidar": "^3.5.1",
    "express": "^4.17.1",
    "figlet": "^1.5.0",
    "js-yaml": "^4.1.0",
    "jssha": "^3.2.0",
    "ncu": "^0.2.1",
    "node-dir": "^0.1.17",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "underscore": "^1.13.1"
  },
  "devDependencies": {
    "concurrently": "^6.1.0",
    "npm-check-updates": "^11.5.11",
    "yarn": "^1.22.10"
  }
}
