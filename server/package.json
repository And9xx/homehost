{
  "name": "homehost-server",
  "version": "1.9.0",
  "private": true,
  "author": "ridhwaans",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=dev nodemon server",
    "client-dev": "cd ../client && npm start",
    "start-dev": "concurrently --kill-others-on-fail \"npm run dev\" \"npm run client-dev\"",
    "prestart": "cd ../client && npm install && npm run build",
    "start": "NODE_ENV=prod node server"
  },
  "dependencies": {
    "@prisma/client": "^3.4.2",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "chokidar": "^3.5.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "figlet": "^1.5.2",
    "get-audio-duration": "^3.0.0",
    "qs": "^6.10.1",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.0.2",
    "prisma": "^3.4.2"
  }
}
